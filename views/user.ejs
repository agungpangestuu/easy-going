<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Role</th>
      </tr>
      <tr>
        <%dataUsers.forEach(user=>{%>
          <td><%=user.id%></td>
          <td><%=user.email%></td>
          <td><%=user.role%></td>
          <%if(user.role == 'Penjual'){%>
            <td><a href="/users/<%=user.id%>/addCarTobiding">Add Car</a></td>
          <%}%>
          <%if(user.role == 'Renter'){%>
            <td><a href="/users/<%=user.id%>/bid">Bid!</a></td>
          <%}%>
          
          
      
      </tr>
        <%})%>
    </table>
  </body>
</html>